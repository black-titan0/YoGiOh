<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>


<StackPane xmlns:fx="http://javafx.com/fxml/1" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
           minWidth="-Infinity"
           prefHeight="680.0" prefWidth="1200.0" xmlns="http://javafx.com/javafx/16"
           fx:controller="view.menus.RegisterMenuView">

    <AnchorPane prefHeight="680.0" prefWidth="1200.0">
        <children>
            <ImageView fitHeight="680.0" fitWidth="1200.0">
                <Image url="@../image/menuBackground.jpg"/>
            </ImageView>
            <TextField fx:id="registerUsernameInput" layoutX="499.0" layoutY="287.0" maxHeight="70" maxWidth="335"
                       prefHeight="25.0" prefWidth="239.0"/>
            <TextField fx:id="registerNicknameInput" layoutX="499.0" layoutY="247.0" maxHeight="70" maxWidth="335"
                       prefHeight="25.0" prefWidth="239.0"/>
            <TextField fx:id="registerPasswordInput" layoutX="544.0" layoutY="319.0" maxHeight="70" maxWidth="335"/>
            <ImageView fitHeight="57.0" fitWidth="142.0" layoutX="548.0" layoutY="371.0"
                       onMouseClicked="#openFirstPage">
                <Image url="@../image/button.jpg"/>
            </ImageView>
            <Text fill="#daa3a3" layoutX="577.0" layoutY="406.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Register"
                  textAlignment="CENTER" wrappingWidth="83.13671875" onMouseClicked="#registerUser">
                <font>
                    <Font name="NPIOuj" size="24.0"/>
                </font>
            </Text>
            <Label layoutX="430.0" layoutY="251.0" text="Username"/>
            <Label layoutX="432.0" layoutY="323.0" text="Password"/>
            <Label layoutX="430.0" layoutY="291.0" prefHeight="17.0" prefWidth="54.0" text="Nickname"/>
            <ImageView fx:id="backButton" fitHeight="57.0" fitWidth="142.0" layoutX="30.0" layoutY="566.0"
                       onMouseClicked="#openFirstPage">
                <Image url="@../image/button.jpg"/>
            </ImageView>
            <Text fill="#daa3a3" layoutX="74.0" layoutY="600.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Back"
                  textAlignment="CENTER" wrappingWidth="54.13671875" onMouseClicked="#openFirstPage">
                <font>
                    <Font name="NPIOuj" size="24.0"/>
                </font>
            </Text>
        </children>
    </AnchorPane>

</StackPane>
